Host *
    User jeroen.opteynde
    IdentityFile ~/.ssh/id_rsa
    TCPKeepAlive yes
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p
    ServerAliveInterval 60
    ControlPersist 30m
    UseRoaming no

Host 192.168.8*
    IdentityFile ~/.ssh/id_rsa_zeno

Host *.internal.vikingco.com
    IdentityFile ~/.ssh/id_rsa_zeno
